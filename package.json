{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Personal website and portfolio,",
  "author": "Elijah Kotyluk",
  "private": true,
  "engines": {
    "node": "10.x.x"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "NODE_ENV=development nuxt build",
    "build:production": "NODE_ENV=production nuxt build",
    "start": "HOST=0.0.0.0 nuxt start",
    "generate": "nuxt generate",
    "deploy": "yarn clean && yarn build && gcloud app deploy --project elijah-kotyluk",
    "deploy:production": "yarn clean && yarn build:production && gcloud app deploy --project elijah-kotyluk",
    "clean": "rm -rf .nuxt dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/vue-fontawesome": "^0.1.2",
    "@nuxtjs/axios": "^5.0.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "nuxt": "^2.0.0",
    "vuetify": "^1.2.4"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
